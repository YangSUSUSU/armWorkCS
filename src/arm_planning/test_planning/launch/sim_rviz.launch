
<launch>
  <arg name="model" default="humanoid"/>

  <param name="robot_description" command="$(find xacro)/xacro --inorder '$(find test_planning)/model/$(arg model).urdf'" />
  <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" />
  
  <node name="rviz" pkg="rviz" type="rviz" args="-d $(find test_planning)/rviz/urdf.rviz" />
  
    
</launch>